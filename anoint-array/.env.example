# ANOINT Array Environment Variables Template
# Copy this file to .env.local and fill in your actual values
# NEVER commit .env.local to version control

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Admin Configuration
SUPABASE_ADMIN_EMAIL=admin@yourdomain.com
SUPABASE_ADMIN_PASSWORD=your_secure_admin_password

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# Anthropic Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key

# Canada Post API
CANPOST_DEV_USERNAME=your_canpost_dev_username
CANPOST_DEV_PASSWORD=your_canpost_dev_password
CANPOST_PROD_USERNAME=your_canpost_prod_username
CANPOST_PROD_PASSWORD=your_canpost_prod_password

# UPS API
UPS_CLIENT_ID=your_ups_client_id
UPS_SECRET=your_ups_secret

# PayPal API
PAYPAL_CLIENT_ID_SANDBOX=your_paypal_sandbox_client_id
PAYPAL_CLIENT_SECRET_SANDBOX=your_paypal_sandbox_secret
PAYPAL_CLIENT_ID_LIVE=your_paypal_live_client_id
PAYPAL_SECRET_LIVE=your_paypal_live_secret

# Stripe API
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_PUBLISH_KEY=your_stripe_publishable_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Resend Email API
RESEND_API_KEY=your_resend_api_key

# Now Payments API
NOWPAYMENTS_PUBLIC_KEY=your_nowpayments_public_key
NOWPAYMENTS_API_KEY=your_nowpayments_api_key

# Fourthwall API
FOURTHWALL_API_USERNAME=your_fourthwall_api_username
FOURTHWALL_API_PASSWORD=your_fourthwall_api_password
FOURTHWALL_STOREFRONT_TOKEN=your_fourthwall_storefront_token
FOURTHWALL_API_URL=https://api.fourthwall.com/v1
FOURTHWALL_SHOP_URL=https://fourthwall.com/shop/your-shop-name

# Application URLs
NEXT_PUBLIC_BASE_URL=https://yourdomain.com
NEXTAUTH_URL=https://yourdomain.com

# Environment
NODE_ENV=production